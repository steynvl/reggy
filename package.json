{
  "name": "Reggy",
  "version": "1.0.0",
  "author": "Steyn van Litsenborgh",
  "description": "A regular expression generator.",
  "angular-cli": {},
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "ng": "ng",
    "compileServer": "tsc -p server",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "buildProduction": "ng build -aot -prod && tsc -p server",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "postinstall": "tsc -p server && ng build -aot -prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cli": "~1.7.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@auth0/angular-jwt": "^1.0.0-beta.9",
    "@types/jasmine": "~2.8.3",
    "@types/node": "~6.0.60",
    "angular5-data-table": "^0.5.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "bootswatch": "^4.0.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "core-js": "^2.4.1",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jquery-highlighttextarea": "^3.2.1",
    "jsonwebtoken": "^8.1.1",
    "morgan": "^1.9.0",
    "ng2-dnd": "^5.0.2",
    "ngx-clipboard": "^10.0.0",
    "ngx-highlightjs": "^1.4.1",
    "nodemailer": "^4.6.5",
    "popper": "^1.0.1",
    "rxjs": "^5.5.6",
